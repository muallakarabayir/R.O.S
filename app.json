{
    "expo": {
        "name": "busApp",
        "slug": "busApp",
        "version": "1.0.0",
        "entryPoint": "./app/index.tsx",
        "orientation": "portrait",
        "icon": "./assets/images/icon.png",
        "scheme": "busapp",
        "userInterfaceStyle": "automatic",
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            }
        },
        "web": {
            "bundler": "metro",
            "output": "server",
            "favicon": "./assets/images/favicon.png"
        },
        "plugins": [
            [
                "expo-router",
                {
                    "origin": "https://ros.dev/"
                }
            ]
        ],
        "experiments": {
            "typedRoutes": true
        },
        "experimental": {
      "newArchEnabled": true
    }
        
    }
}