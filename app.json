{
    "expo": {
        "name": "busApp",
        "slug": "busApp",
        "version": "1.0.0",
        "entryPoint": "./app/index.tsx",
        "orientation": "portrait",
        "icon": "./assets/images/icon.png",
        "scheme": "busapp",
        "userInterfaceStyle": "automatic",
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            }
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/images/favicon.png"
        },
        "plugins": [
            "expo-router", [
                "expo-splash-screen",
                {
                    "image": "./assets/images/splash-icon.png",
                    "imageWidth": 200,
                    "resizeMode": "contain",
                    "backgroundColor": "#ffffff"
                }
            ]
        ],
        "experiments": {
            "typedRoutes": true
        },
        "experimental": {
      "newArchEnabled": true
    }
        
    }
}